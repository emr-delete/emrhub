-- Scripts:

local function LLVLLI_fake_script() -- LoginButton.LocalScript 
	local script = Instance.new('LocalScript', LoginButton)

	local password = script.Parent.Parent.Parent.box.Password
	local main = script.Parent.Parent.Parent.Parent.main
	local login = script.Parent.Parent.Parent.Parent.Login
	
	main.Visible = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if password.Text == "gGoB8ZtrDDRelH0rNP" or 
			password.Text == "gGoB8ZtrDDRelH0rNPFc" then
			login.Visible = false
			main.Visible = true
	
	
			
		end
	end)
end
coroutine.wrap(LLVLLI_fake_script)()
local function KZHI_fake_script() -- main.LocalScript 
	local script = Instance.new('LocalScript', main)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Frame10.Visible = true
	end)
end
coroutine.wrap(KZHI_fake_script)()
